:{"line":67,"column":116},"source":"src/index.js","original":{"line":50,"column":17}},{"generated":{"line":67,"column":123},"source":"src/index.js","original":{"line":48,"column":48}},{"generated":{"line":67,"column":134},"source":"src/index.js","original":{"line":48,"column":17}},{"generated":{"line":68,"column":0},"source":"src/index.js","original":{"line":48,"column":17}},{"generated":{"line":68,"column":3},"source":"src/index.js","original":{"line":48,"column":4}},{"name":"window","generated":{"line":69,"column":0},"source":"src/index.js","original":{"line":52,"column":4}},{"name":"window","generated":{"line":69,"column":2},"source":"src/index.js","original":{"line":52,"column":4}},{"generated":{"line":69,"column":8},"source":"src/index.js","original":{"line":52,"column":10}},{"name":"location","generated":{"line":69,"column":9},"source":"src/index.js","original":{"line":52,"column":11}},{"generated":{"line":69,"column":17},"source":"src/index.js","original":{"line":52,"column":4}},{"name":"href","generated":{"line":69,"column":18},"source":"src/index.js","original":{"line":52,"column":20}},{"generated":{"line":69,"column":22},"source":"src/index.js","original":{"line":52,"column":4}},{"generated":{"line":69,"column":25},"source":"src/index.js","original":{"line":52,"column":27}},{"generated":{"line":69,"column":37},"source":"src/index.js","original":{"line":52,"column":4}},{"generated":{"line":70,"column":0},"source":"src/index.js","original":{"line":53,"column":3}}],"sources":{"src/index.js":" //怎么生成10000条数据？  Array.from  用内存来放，才10000条，\r\n        //和一个4kyu的题目很像，分页 originNews 分成200个数组（二维） [[50][50]]\r\n       // 10000 条数据？  Array.from  内存\r\n  // 4yu  10000 originNews 200个数组 [[50],[50]]\r\n  let page = 0;  // 页码,这里是每页固定数量，有样式，触底刷出下一页\r\n  let pageNum = Math.ceil(window.innerHeight / 50);\r\n  const chunk = (arr, size) => Array.from({\r\n    length: Math.ceil(arr.length / size)\r\n  }, (v, i) => \r\n    arr.slice(i * size, i * size + size)\r\n  )\r\n    \r\n  const originNews = Array.from({\r\n    length: 1000\r\n  }, (v, k) => ({\r\n    content: `新闻${k}`\r\n  }))\r\n  let news = chunk(originNews, pageNum) // 当前页需要的数据\r\n  // console.log(news)\r\n\r\n  const element = document.querySelector('.news'); // 代孕的\r\n  const loadObserver = new IntersectionObserver((entries) => {\r\n    // if ()\r\n    // console.log(entries);\r\n    if (entries[0].intersectionRatio <= 0) {\r\n      return;\r\n    }\r\n    const hasPrePage = page != 0; // page 当前页面, \r\n    const hasNextPage = page != news.length - 1 // 最后一页？ \r\n    const now = news[page]\r\n    const pre = hasPrePage ? news[page - 1]:[];\r\n    const next = hasNextPage ? news[page + 1]: [];\r\n\r\n    render(pre.length, [...pre, ...now, ...next]);\r\n    page = entries[0].target.className == 'news-footer' || page == 0\r\n      ? (hasNextPage? page + 1: page)\r\n      : (hasPrePage? page - 1: page)\r\n    \r\n  }, {\r\n    threshold:[1]\r\n  }) // 监听\r\n\r\n  loadObserver.observe(document.querySelector('.news-header'));\r\n  loadObserver.observe(document.querySelector('.news-footer'));\r\n // 建议函数内的代码不要超过10行 封装\r\n  function render(last, data) {\r\n    element.innerHTML = '';\r\n    data.forEach((i, v) => element.innerHTML += v == last \r\n      ? `<div id=\"news-href\">${i.content}</div>`\r\n      :`<div>${i.content}</div>`\r\n    )\r\n    window.location.href = '#news-href';\r\n  }\r\n"},"lineCount":null}},"error":null,"hash":"42b9f0b082a4236faaee46bc5f26fd76","cacheData":{"env":{}}}