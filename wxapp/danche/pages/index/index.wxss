/* 微信小程序中页面最大的盒子就是page */
page{
  display: flex;
  flex-direction: column;
  height: 100%;
}
.map-box {
  width: 100%;
  height: 100%;
  overflow: hidden;
  flex: 1;
}
.main-btn {
  background-color: #ffffff;
  padding: 20rpx;
}
.main-text {
  height: 100rpx;
  margin: 0 auto;
  color: #f7f4f4;
  background-color: #161618;
  font-size: 32rpx;
  border-radius: 4rpx;
  display:flex;
  justify-content: center;
  align-items:center;
  text-align: center;
}
